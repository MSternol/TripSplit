@page
@model TripSplit.Web.Areas.Identity.Pages.Account.LogoutModel
@{
    Layout = "/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Wylogowanie";
}
<section class="ts-card">
    <form method="post">
        <p>Czy na pewno chcesz się wylogować?</p>
        <button class="btn btn--accent" type="submit">Wyloguj</button>
        <a class="btn btn--ghost" asp-area="" asp-controller="Trips" asp-action="Index">Anuluj</a>
    </form>
</section>
